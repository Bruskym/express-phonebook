<% if(errors.length > 0){ %>
    <div class="row">
        <div class="col my-3">
            <div class="alert alert-danger">
                <ul>
                <% errors.forEach(error => { %>
                    <li><%= error %></li>
                <% }) %>
                </ul>
            </div>
        </div>
    </div>
<% } %>